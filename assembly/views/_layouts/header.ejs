<body>
  <div id="wrapper">
    <!-- Header -->
    <div id="header">
      <div id="logo">
        <a href="/Home"><img src="/images/Logo.svg" />
          <img class="mainLogoText" src="/images/Component 5 â€“ 2.svg" style="margin-left: 0.5em;" /></a>
      </div>
      <div id="headerButtons">
        <% if (cart && cart.length > 0) { %>
        <div class="fa-2x">
          <a href="/cart/checkout" class="cta" style="margin-right: 0.5em;">
            <span class="fa-layers fa-fw">
              <i class="fas fa-shopping-cart"></i>
              <span class="fa-layers-counter fa-lg"><%= _.sumBy(cart, p => p.quantity) %></span>
            </span>
          </a>
        </div>
        <% } if (user) { %>
        <a href="/users/logout">
          <div class="discordAvatar">
            <img src="https://cdn.discordapp.com/avatars/<%= user.discordId %>/<%= user.avatar %>.png" />
            <div>LOG OUT</div>
          </div>
        </a>
        <% } else { %>
        <a href="/auth/discord" class="cta">LOG IN</a>
        <% } %>
      </div>
      <div id="search">
        <form action="/products/search" method="GET" style="display: flex;width: 100%;">
          <input name="q" id="productSearch" placeholder="Search products" type="text" class="form-control"
            style="height:100%" />
          <button id="searchButton" type="submit"><img id="searchImg" src="/images/searchImg.svg"></button>
        </form>
      </div>
    </div>
    <!-- Header End-->
